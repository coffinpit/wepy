<script>
import wepy from 'wepy'
import 'wepy-async-function'

import { setStore } from 'wepy-redux'
import configStore from './store'

import HttpRequest from './service/HttpRequest'

const store = configStore()
setStore(store)

export default class extends wepy.app {
  config = {
    pages: [
      'pages/start',
      'pages/login',
      'pages/index',
      'pages/detail',
      'pages/cart',
      'pages/user',
      'pages/hot',
      'pages/search'
    ],
    window: {
      backgroundTextStyle: 'dark',
      navigationBarBackgroundColor: '#fc5e43',
      navigationBarTitleText: 'WeChat',
      navigationBarTextStyle: 'white'
    },
    tabBar: {
      color: '#282626',
      selectedColor: '#fc5e44',
      backgroundColor: '#f8f8f8',
      list: [{
        pagePath: 'pages/index',
        iconPath: 'image/index-default.png',
        selectedIconPath: 'image/index-active.png',
        text: '首页'
      }, {
        pagePath: 'pages/cart',
        iconPath: 'image/category-default.png',
        selectedIconPath: 'image/category-active.png',
        text: '分类'
      }, {
        pagePath: 'pages/cart',
        iconPath: 'image/cart-default.png',
        selectedIconPath: 'image/cart-active.png',
        text: '购物车'
      }, {
        pagePath: 'pages/user',
        iconPath: 'image/user-default.png',
        selectedIconPath: 'image/user-active.png',
        text: '个人中心'
      }]
    }
  }

  globalData = {
    userInfo: null
  }

  constructor () {
    super()
    this.use('requestfix')
    this.intercept('request', {
      config (p) {
        p.timestamp = +new Date()
        wepy.showLoading({
          title: '加载中',
          icon: 'loading'
        })
        console.log('config request: ', p)
        return p
      },
      success (p) {
        console.log('request success: ', p)
        wepy.hideLoading()
        return p
      },
      fail (p) {
        console.log('request fail: ', p)
        wepy.hideLoading()
        wepy.showToast({
          title: '加载失败',
          icon: 'none',
          image: '../image/cancel.png'
        })
        return p
      },
      complete (p) {
        console.log('request complete: ', p)
      }
    })
  }

  onLaunch() {
    this.testAsync()
  }

  sleep (s) {
    return new Promise((resolve, reject) => {
      setTimeout(() => {
        resolve('promise resolved')
      }, s * 1000)
    })
  }

  async testAsync () {
    const data = await this.sleep(3)
    console.log(data)
  }

  getUserInfo(cb) {
    const that = this
    if (this.globalData.userInfo) {
      return this.globalData.userInfo
    }
    wepy.getUserInfo({
      success (res) {
        that.globalData.userInfo = res.userInfo
        cb && cb(res.userInfo)
      },
      fail () {
        wepy.showModal({
          title: '警告',
          content: '登录失败',
          success: (res) => {
            if (res.confirm) {
              console.log('用户已确认')
            }
          }
        })
      }
    })
  }

  HttpRequest = new HttpRequest()
}
</script>
