/* -- base color -- */
/* -- base fontSize -- */
/* -- base lineHeight -- */
/* -- Mixin -- */
.container {
  min-height: calc(100vh);
  display: block;
}
.user-logo {
  padding: 20rpx 30rpx 30rpx;
  background: #ec3d3a;
  width: 100%;
  box-sizing: border-box;
  height: 160rpx;
}
.user-logo .userinfo-avatar {
  width: 100rpx;
  height: 100rpx;
  border: 2px #ffffff solid;
  border-radius: 50%;
  overflow: hidden;
  float: left;
}
.user-logo .userinfo-nickname {
  float: left;
  width: 320rpx;
  height: 100rpx;
  overflow: hidden;
  padding-top: 30rpx;
  color: #ffffff;
  line-height: 48rpx;
  margin-left: 40rpx;
  font-size: 32rpx;
}
.user-logo .userinfo-nickname text {
  display: block;
}
.user-logo .telephone {
  float: right;
  color: #ffffff;
  margin-top: 38rpx;
}
.user-logo .blank-button {
  float: right;
  border: 1px #ffffff solid;
  color: #ffffff;
  margin-top: 38rpx;
}
.user-logo .validation {
  font-size: 28rpx;
}
.user-logo .user-setting {
  padding: 10rpx 24rpx 8rpx 12rpx;
  border-radius: 60rpx;
  color: #ffffff;
  font-size: 24rpx;
  float: right;
  border: 1px #ffffff solid;
  margin-top: 30rpx;
}
.user-logo .user-setting text {
  display: inline-block;
  height: 30rpx;
  padding-left: 50rpx;
  background: url(data:image/png;base64,) no-repeat 10rpx center;
  background-size: contain;
}
.user-level {
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  background: #0f1319;
}
.user-level .iconfont {
  color: #999999;
  font-size: 70rpx;
  padding-right: 20rpx;
  margin-right: 30rpx;
  border-right: 1px #999999 solid;
  display: inline-block;
  vertical-align: middle;
}
.user-level .iconfont.vip {
  color: #ec3d3a;
}
.user-level image {
  width: 35rpx;
  height: 31rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}
.user-level view {
  width: 635rpx;
  display: inline-block;
}
.user-level .title {
  color: #ffffff;
  font-size: 28rpx;
}
.user-level .default-button {
  padding: 0 20rpx;
  float: right;
  font-size: 28rpx;
}
.setting-container {
  margin-top: 20rpx;
}
.setting-container .default-button {
  padding: 20rpx 0;
  margin: 50rpx 20rpx 20rpx;
}
.setting-layer {
  border-top: 1px #eeeeee solid;
  font-size: 28rpx;
  color: #333333;
  width: 100%;
  padding: 20rpx 20rpx 20rpx 30rpx;
  box-sizing: border-box;
  background: #ffffff;
}
.setting-layer:first-child {
  border-top: none;
}
.setting-layer .icon-me_fw {
  font-size: 60rpx;
}
.setting-layer .iconfont {
  font-size: 56rpx;
  color: #d9d9d9;
  margin-right: 20rpx;
  display: inline-block;
  width: 60rpx;
  height: 65rpx;
  line-height: 65rpx;
}
.setting-layer .iconfont.icon-rightsj {
  color: #333333;
}
.setting-layer text {
  vertical-align: middle;
}
.setting-layer .icon-rightsj {
  margin: 6rpx 0 0;
  float: right;
  font-size: 40rpx;
}
.setting-layer .notice {
  float: right;
  margin: 16rpx 20rpx 0 0;
  width: 40rpx;
  height: 40rpx;
  background: #ec3d3a;
  border-radius: 50%;
  font-size: 24rpx;
  color: #ffffff;
  text-align: center;
  line-height: 40rpx;
}
.setting-layer .order-list {
  padding: 20rpx 0;
  width: 25%;
  border-right: 1px #eeeeee solid;
  box-sizing: border-box;
  text-align: center;
  display: inline-block;
  height: 126rpx;
}
.setting-layer .order-list:last-child {
  border-right: none;
}
.setting-layer .order-list text {
  display: block;
  line-height: 44rpx;
}
.setting-layer .order-list text:first-child {
  height: 44rpx;
}
.order-layer {
  padding: 20rpx 0;
}
