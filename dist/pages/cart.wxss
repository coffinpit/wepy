@import "./../components/orderitem.wxss";
view {
  box-sizing: border-box;
}
.container {
  padding-bottom: 120rpx;
}
.cart-status {
  background: #ffffff;
  border-bottom: 2px #fc5e43 solid;
  padding: 30rpx 20rpx;
  display: flex;
  width: 100%;
  justify-content: space-between;
}
.cart-list {
  margin-top: 20rpx;
  width: 100%;
}
.cart-list .content {
  margin-bottom: 0;
}
.cart-list .iconfont {
  font-size: 80rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}
.vip-tip {
  width: 100%;
  margin: 10rpx 0;
  background: #fff;
  padding: 10rpx 20rpx;
}
.vip-tip .iconfont {
  font-size: 60rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
