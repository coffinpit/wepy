@import "./../components/menu.wxss";
@import "./../components/counter.wxss";
@import "./../components/bottombar.wxss";
.container {
  overflow: hidden;
  padding-bottom: 116rpx;
}
.detail-info {
  position: relative;
  height: 575rpx;
  width: 100%;
}
.detail-info .detail-img {
  width: 525rpx;
  height: 525rpx;
  background: #fff;
  padding: 20px 0;
  box-sizing: border-box;
  position: absolute;
  left: 10px;
  top: 10px;
  z-index: 9;
  overflow: hidden;
  box-shadow: 2px 2px 2px #eee;
}
.detail-info .detail-img image {
  width: 505rpx;
  height: 505rpx;
}
.detail-info .detail-dots {
  position: absolute;
  top: 14px;
  left: 520rpx;
  z-index: 999;
}
.detail-info .detail-dots .detail-dots-gray,
.detail-info .detail-dots .detail-dots-orange {
  width: 7px;
  height: 7px;
  border-radius: 50%;
  background: #eee;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 999;
}
.detail-info .detail-dots .detail-solid {
  width: 1px;
  height: 15px;
  background: #fc5e44;
  position: absolute;
  left: 3px;
  top: 4px;
  z-index: 9999;
}
.detail-info .detail-dots .detail-dots-orange {
  background: #fff;
  top: 16px;
}
.detail-info .detail-txt {
  position: absolute;
  right: 10px;
  top: 25px;
  width: 225rpx;
  height: 525rpx;
  background: #fff;
  box-shadow: 2px 2px 2px #eee;
  z-index: 998;
  font-size: 32rpx;
  line-height: 40rpx;
}
.detail-info .detail-txt text {
  display: block;
}
.detail-info .detail-txt .detail-txt-layer {
  background: url('data:image/png;base64,');
  background-size: cover;
  width: 100%;
  height: 140rpx;
  color: #fff;
  position: relative;
}
.detail-info .detail-txt .detail-txt-layer .detail-txt-content {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 998;
  text-align: center;
}
.detail-info .detail-txt .detail-txt-price {
  padding: 16rpx 30rpx 20rpx;
  text-align: center;
  color: #333;
  border-bottom: 1px #eee solid;
}
.detail-info .detail-txt .detail-txt-price:last-child {
  border-bottom: none;
}
.detail-info .detail-txt .detail-txt-price .detail-link {
  border-bottom: 1px #fc5e44 solid;
  color: #fc5e44;
  display: inline-block;
}
.detail-info .detail-txt .detail-txt-price.express {
  text-align: left;
}
.detail-info .detail-txt .detail-txt-price.express .tips {
  color: #999;
  font-size: 28rpx;
}
.detail-info .detail-txt .detail-txt-price .default-button {
  margin-top: 5px;
  padding: 6rpx 0;
}
.content {
  padding: 20rpx 30rpx;
  margin-top: 10px;
  margin-bottom: 0;
}
.detail-title {
  border-bottom: 1px #eee solid;
  padding: 10rpx 0 20rpx;
  margin-bottom: 20rpx;
}
.detail-collect {
  display: flex;
  align-items: stretch;
}
.detail-collect .detail-collect-icon {
  width: 18%;
  text-align: center;
}
.detail-collect .detail-collect-icon text {
  display: block;
}
.detail-collect .detail-collect-icon image {
  width: 80rpx;
  height: 69rpx;
}
.detail-collect .detail-collect-icon .collect-txt {
  color: #666;
  font-size: 28rpx;
}
.detail-collect .detail-collect-icon.collected .iconfont,
.detail-collect .detail-collect-icon.collected .collect-txt {
  color: #fc5e43;
}
.detail-collect .detail-collect-user {
  width: 80%;
  margin-left: 2%;
}
.detail-collect .detail-collect-user .detail-collect-detail {
  overflow: hidden;
}
.detail-collect .detail-collect-user .detail-collect-detail .iconfont {
  font-size: 28rpx;
  color: #999;
  float: right;
  margin-top: 18rpx;
}
.detail-collect .detail-collect-user .detail-collect-detail .section {
  margin-top: 10rpx;
}
.detail-collect .detail-collect-user .detail-collect-list {
  margin-top: 10rpx;
  min-height: 84rpx;
}
.detail-collect .detail-collect-user .detail-collect-list view {
  width: 74rpx;
  height: 74rpx;
  background: #eee;
  margin-right: 5px;
  display: inline-block;
  overflow: hidden;
}
.detail-collect .detail-collect-user .detail-collect-list view:last-child {
  margin-left: 0;
}
.detail-collect .detail-collect-user .detail-collect-list view image {
  width: 100%;
  height: 100%;
}
.detail-collect .un-collect {
  padding: 10rpx 20rpx;
  color: #fff;
  background: #ffa336;
  font-size: 24rpx;
}
.detail-collect .un-collect.collect {
  background: #fc5e43;
}
.detail-collect .icon-leftsj {
  color: #ffa336;
  vertical-align: middle;
  margin-right: -10rpx;
  font-size: 30rpx;
}
.detail-collect .icon-leftsj.collect {
  color: #fc5e43;
}
.detail-container {
  margin-bottom: 10px;
}
.detail-container .section {
  margin-bottom: 5px;
}
.detail-container .detail-goods {
  display: flex;
  padding: 4rpx 0 4rpx 28rpx;
  font-size: 24rpx;
  color: #333;
  line-height: 36rpx;
}
.detail-container .detail-goods .detail-goods-title {
  width: 20%;
}
.detail-container .detail-goods .detail-goods-content {
  width: 80%;
}
.popup {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999999;
}
.popup__content {
  position: fixed;
  bottom: -100%;
  left: 0;
  transition: 0.5s all ease-out;
  width: 100%;
  min-height: 500rpx;
  background: #ffffff;
  z-index: 9999999;
}
.popup__content.active {
  bottom: 0;
}
.popup_close {
  padding: 20rpx 40rpx;
  text-align: right;
}
.popup_layer {
  border-top: 1px #eee solid;
  padding: 20rpx;
  display: flex;
}
.popup_layer .popup_image {
  border: 1px #eee solid;
  width: 200rpx;
  height: 200rpx;
  overflow: hidden;
}
.popup_layer .popup_image image {
  width: 100%;
  height: 100%;
}
.popup_layer .popup_goods {
  padding-left: 44rpx;
  padding-top: 20rpx;
  width: 550rpx;
  box-sizing: border-box;
}
.popup_layer .popup_goods .link {
  display: block;
}
.popup_layer .popup_goods .link.inline {
  float: left;
  margin-top: 52rpx;
}
.popup_layer .popup_goods .calculate {
  margin-top: 20rpx;
  margin-right: 20rpx;
  float: left;
}
.popup_layer .popup_goods .popup_goods_box {
  display: flex;
}
.popup_layer .popup_goods .popup_goods_box .popup_goods_left {
  width: 20%;
  white-space: nowrap;
}
.popup_layer .popup_goods .popup_goods_box .popup_goods_right {
  width: 80%;
}
.popup_layer .goods-name {
  width: 70%;
  font-size: 28rpx;
}
.popup_layer .goods-num {
  width: 30%;
  font-size: 28rpx;
}
.popup_layer .none {
  color: #999;
}
.popup_layer radio {
  float: right;
}
.disable {
  background: #999;
}
