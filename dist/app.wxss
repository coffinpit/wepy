/* -- Mixin -- */
.goods-promot {
  position: relative;
  margin-top: 8rpx;
  height: 44rpx;
}
.goods-promot .goods-promot-image {
  width: 40rpx;
  height: 40rpx;
  border: 1px #ec3d3a solid;
  border-radius: 50%;
  background: #ffffff;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 99;
}
.goods-promot .goods-promot-image image {
  width: 32rpx;
  height: 28rpx;
  position: absolute;
  left: 6rpx;
  top: 6rpx;
}
.goods-promot .goods-info {
  background: #eac259;
  color: #ffffff;
  font-size: 24rpx;
  line-height: 36rpx;
  height: 36rpx;
  padding: 0 16rpx;
  position: absolute;
  border-radius: 4rpx;
  top: 4rpx;
  left: 0;
  z-index: 1;
  min-width: 100rpx;
}
.goods-promot .goods-end {
  position: absolute;
  right: -28rpx;
  top: 0;
  height: 40rpx;
}
.goods-promot .goods-end image {
  width: 30rpx;
  height: 36rpx;
  float: left;
}
