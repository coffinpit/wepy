/* -- base color -- */
/* -- base fontSize -- */
/* -- base lineHeight -- */
/* -- Mixin -- */
page {
  background: #f8f8f8;
}
.overflow {
  overflow: hidden;
  clear: both;
}
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.content {
  padding: 20rpx;
  background: #ffffff;
  margin-bottom: 10px;
  width: 100%;
  box-sizing: border-box;
}
.content:last-child {
  margin-bottom: 0;
}
.default-button {
  background: #ec3d3a;
  color: #ffffff;
  font-size: 28rpx;
  line-height: 48rpx;
  border-radius: 0;
  margin: 0;
}
.default-button-hover {
  background: #ff2400;
}
.blank-button {
  border: 1px #ec3d3a solid;
  color: #ec3d3a;
  font-size: 28rpx;
  line-height: 44rpx;
  border-radius: 0;
  margin: 0;
  background: transparent;
  padding: 0 20rpx;
}
.blank-button-hover {
  background: #ff2400;
  color: #ffffff;
}
.white-button {
  border: 1px #ec3d3a solid;
  background: #ffffff;
  color: #ec3d3a;
  font-size: 28rpx;
  line-height: 48rpx;
  border-radius: 0;
  margin: 0;
  padding: 0 20rpx;
}
.size-full {
  width: 100%;
  line-height: 116rpx;
  font-size: 32rpx;
}
/* 预设字体定义 color/font-size/line-height */
.title {
  color: #333333;
  font-size: 32rpx;
  line-height: 44rpx;
}
.font-normal {
  color: #333333;
  font-size: 28rpx;
  line-height: 44rpx;
}
.font-small {
  color: #333333;
  font-size: 24rpx;
  line-height: 36rpx;
}
.section {
  color: #333333;
  font-size: 28rpx;
  line-height: 44rpx;
}
.link {
  color: #ec3d3a !important;
}
.font-bold {
  font-weight: 700;
}
.tips {
  font-size: 24rpx;
  color: #999999;
}
.through {
  text-decoration: line-through;
}
button::after {
  border: none;
}
